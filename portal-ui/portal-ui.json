{
    "$schema": "https://schema.management.azure.com/schemas/0.1.2-preview/CreateUIDefinition.MultiVm.json#",
    "handler": "Microsoft.Azure.CreateUIDef",
    "version": "0.1.2-preview",
    "parameters": {
        "basics": [
            {
                "label": "Function App Name",
                "name": "FunctionAppName",
                "type": "Microsoft.Common.TextBox",
                "visible": true,
                "constraints": {
                    "required": true,
                    "regex": "^[a-zA-Z0-9-]*$",
                    "validationMessage": "The name can contain only letters,numbers, and hyphens."
                }
            },
            {
                "label": "Host Pool Name",
                "name": "HostPoolName",
                "type": "Microsoft.Common.TextBox",
                "visible": true,
                "constraints": {
                    "required": true,
                    "regex": "^[a-zA-Z0-9-]*$",
                    "validationMessage": "The name can contain only letters,numbers, and hyphens."
                }
            },
            {
                "label": "Target Session Host Count",
                "name": "TargetSessionHostCount",
                "type": "Microsoft.Common.TextBox",
                "defaultValue": "10",
                "visible": true,
                "constraints": {
                    "required": true,
                    "regex": "^[0-9]*$",
                    "validationMessage": "The value must be a number."
                }
            },
            {
                "label": "Session Host Name Prefix",
                "name": "SessionHostNamePrefix",
                "type": "Microsoft.Common.TextBox",
                "visible": true,
                "constraints": {
                    "required": true,
                    "regex": "^[a-zA-Z0-9-]{1,10}$",
                    "validationMessage": "The value must be between 1 and 10 characters and can contain only letters,numbers, and hyphens."
                }
            },
            {
                "label": "Session Host Template URI or Template Spec Resource Id",
                "name": "SessionHostTemplate",
                "type": "Microsoft.Common.TextBox",
                "defaultValue": "https://github.com/WillyMoselhy/AVDReplacementPlans/blob/e28275100ee5a1645c70f0c75e10269f734f06a0/SampleSessionHostTemplate/sessionhost.json",
                "visible": true,
                "constraints": {
                    "required": true,
                    "validationMessage": "The value must be a valid URL."
                }
            },
            {
                "label": "AD Organizational Unit Path",
                "name": "ADOrganizationalUnitPath",
                "type": "Microsoft.Common.TextBox",
                "visible": true,
                "constraints": {
                    "required": true,
                    "validationMessage": "The value must be an OUs Distinguished name(DN)."
                }
            },
            {
                "label": "Subnet Id",
                "name": "SubnetId",
                "type": "Microsoft.Common.TextBox",
                "visible": true,
                "constraints": {
                    "required": true,
                    "validationMessage": "The value must be subnet resource Id."
                }
            },
            {
                "label": "Storage Account Name",
                "name": "StorageAccountName",
                "type": "Microsoft.Common.TextBox",
                "visible": true,
                "constraints": {
                    "required": true,
                    "regex": "^[a-z0-9]{3,24}$",
                    "validationMessage": "The value must be between 3 and 24 characters and can contain only lowercase letters and numbers."
                }
            },
            {
                "label": "Log Analytics Workspace Name",
                "name": "LogAnalyticsWorkspaceName",
                "type": "Microsoft.Common.TextBox",
                "visible": true,
                "constraints": {
                    "required": true,
                    "validationMessage": "The value must be a valid Log Analytics Workspace name."
                }
            }
